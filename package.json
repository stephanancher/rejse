{
  "name": "korselsregnskab",
  "private": true,
  "version": "0.0.1",
  "main": "dist-electron/main.js",
  "description": "Kørselsregnskab App",
  "author": "User",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron": "wait-on tcp:5173 && cross-env NODE_ENV=development electron .",
    "electron:build": "tsc -p electron && vite build && electron-builder",
    "dev:electron": "concurrently -k \"tsc -p electron -w\" \"vite\" \"npm run electron\""
  },
  "build": {
    "appId": "com.korselsregnskab.app",
    "productName": "Kørselsregnskab",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@types/geojson": "^7946.0.16",
    "@types/leaflet": "^1.9.21",
    "exceljs": "^4.4.0",
    "html2canvas": "1.4.1",
    "ini": "^6.0.0",
    "leaflet": "^1.9.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^5.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/ini": "^4.1.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.2.1",
    "electron-builder": "^26.7.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  }
}
